<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="876.0" style="-fx-background-color: #484d4c;" stylesheets="@style.css"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.controller.UpdateEmployeeInfo">
    <children>
        <VBox layoutX="362.0" layoutY="63.0" prefHeight="385.0" prefWidth="269.0" spacing="20.0">
            <children>
                <TextField fx:id="firstName" prefHeight="25.0" prefWidth="403.0" promptText="first name"
                           style="-fx-background-color: transparent; -fx-border-color: #0db9de; -fx-font-family: quicksand; -fx-border-width: 2; -fx-border-radius: 200; -fx-font-size: 14;"/>
                <TextField fx:id="lastName" promptText="last name"
                           style="-fx-background-color: transparent; -fx-border-color: #0db9de; -fx-font-family: quicksand; -fx-border-width: 2; -fx-border-radius: 200; -fx-font-size: 14;"/>
                <TextField fx:id="address" promptText="Place of residence"
                           style="-fx-background-color: transparent; -fx-border-color: #0db9de; -fx-font-family: quicksand; -fx-border-width: 2; -fx-border-radius: 200; -fx-font-size: 14;"/>
                <TextField fx:id="ssn" promptText="Social security number"
                           style="-fx-background-color: transparent; -fx-border-color: #0db9de; -fx-font-family: quicksand; -fx-border-width: 2; -fx-border-radius: 200; -fx-font-size: 14;"/>
                <TextField fx:id="dOb" promptText="YYYY-MM-DD"
                           style="-fx-background-color: transparent; -fx-border-color: #0db9de; -fx-font-family: quicksand; -fx-border-width: 2; -fx-border-radius: 200; -fx-font-size: 14;"/>
                <TextField fx:id="email" promptText="Post E-address"
                           style="-fx-background-color: transparent; -fx-border-color: #0db9de; -fx-font-family: quicksand; -fx-border-width: 2; -fx-border-radius: 200; -fx-font-size: 14;"/>
                <TextField fx:id="userName" promptText="username"
                           style="-fx-background-color: transparent; -fx-border-color: #0db9de; -fx-font-family: quicksand; -fx-border-width: 2; -fx-border-radius: 200; -fx-font-size: 14;"/>
                <TextField fx:id="passWord" promptText="password"
                           style="-fx-background-color: transparent; -fx-border-color: #0db9de; -fx-font-family: quicksand; -fx-border-width: 2; -fx-border-radius: 200; -fx-font-size: 14;"/>
                <TextField fx:id="salary" promptText="Salary" stylesheets="@style.css"/>
                <ChoiceBox fx:id="role" prefHeight="25.0" prefWidth="321.0"
                           style="-fx-background-color: transparent; -fx-border-color: #0db9de; -fx-font-family: quicksand; -fx-border-width: 2; -fx-border-radius: 200;"/>
            </children>
        </VBox>
        <VBox alignment="CENTER" layoutX="703.0" layoutY="474.0" prefHeight="110.0" prefWidth="115.0" spacing="20.0">
            <children>
                <Button mnemonicParsing="false" onAction="#update" prefHeight="37.0" prefWidth="135.0"
                        style="-fx-background-color: #484d4c; -fx-background-radius: 200;" text="Update Info"
                        textFill="#0db9de">
                    <font>
                        <Font name="System Bold Italic" size="16.0"/>
                    </font>
                </Button>
            </children>
        </VBox>
        <VBox layoutX="226.0" layoutY="60.0" prefHeight="385.0" prefWidth="136.0" spacing="28.0">
            <children>
                <Label text="First Name" textFill="#0db9de">
                    <font>
                        <Font name="System Bold Italic" size="16.0"/>
                    </font>
                </Label>
                <Label alignment="CENTER" text="Last name" textFill="#0db9de">
                    <font>
                        <Font name="System Bold Italic" size="16.0"/>
                    </font>
                </Label>
                <Label alignment="CENTER" text="Address" textFill="#0db9de">
                    <font>
                        <Font name="System Bold Italic" size="16.0"/>
                    </font>
                </Label>
                <Label alignment="CENTER" text="SSN" textFill="#0db9de">
                    <font>
                        <Font name="System Bold Italic" size="16.0"/>
                    </font>
                </Label>
                <Label alignment="CENTER" prefHeight="25.0" prefWidth="130.0" text="Date of birthday"
                       textFill="#0db9de">
                    <font>
                        <Font name="System Bold Italic" size="16.0"/>
                    </font>
                </Label>
                <Label alignment="CENTER" text="E-mail" textFill="#0db9de">
                    <font>
                        <Font name="System Bold Italic" size="16.0"/>
                    </font>
                </Label>
                <Label text="User Name" textFill="#0db9de">
                    <font>
                        <Font name="System Bold Italic" size="16.0"/>
                    </font>
                </Label>
                <Label text="Password" textFill="#0db9de">
                    <font>
                        <Font name="System Bold Italic" size="16.0"/>
                    </font>
                </Label>
                <Label stylesheets="@style.css" text="Salary" textFill="#0db9de">
                    <font>
                        <Font name="System Bold Italic" size="16.0"/>
                    </font>
                </Label>
                <Label text="Role" textFill="#0db9de">
                    <font>
                        <Font name="System Bold Italic" size="16.0"/>
                    </font>
                </Label>
            </children>
        </VBox>
        <Button layoutX="47.0" layoutY="510.0" mnemonicParsing="false" onAction="#cancel"
                style="-fx-background-color: #484d4c; -fx-background-radius: 200;" text="Cancel" textFill="#0db9de">
            <font>
                <Font name="System Bold Italic" size="16.0"/>
            </font>
        </Button>
        <Button layoutX="47.0" layoutY="550.0" mnemonicParsing="false" onAction="#backToMain" prefHeight="37.0"
                prefWidth="121.0" style="-fx-background-color: #484d4c; -fx-background-radius: 200;" text="Back to main"
                textFill="#0db9de">
            <font>
                <Font name="System Bold Italic" size="16.0"/>
            </font>
        </Button>
        <Label layoutX="691.0" layoutY="26.0" prefHeight="37.0" prefWidth="102.0" text="Nursing" textFill="#1ad4be">
            <font>
                <Font name="Calisto MT Bold Italic" size="20.0"/>
            </font>
        </Label>
        <Label layoutX="760.0" layoutY="19.0" prefHeight="51.0" prefWidth="102.0" text="Home" textFill="#0db9de">
            <font>
                <Font name="Calisto MT Bold Italic" size="20.0"/>
            </font>
            <graphic>
                <ImageView fitHeight="35.0" fitWidth="35.0">
                    <image>
                        <Image url="@../medicalIcons/stay-at-home.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Label>
        <Label layoutX="60.0" layoutY="11.0" text="Update employee Info" textFill="#0db9de">
            <font>
                <Font name="System Bold Italic" size="20.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
